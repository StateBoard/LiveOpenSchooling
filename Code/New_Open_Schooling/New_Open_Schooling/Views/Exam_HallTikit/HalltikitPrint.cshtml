<body>
    <form id="form1" runat="server">
        <div class="book">

            <div class="page">
                <div class="subpage">

                    <table style="width: 100%; font-weight: bold; height: 70px; padding-top: 10px; line-height: 25px; border-left-style: solid; border-left-color: inherit; border-left-width: 1px; border-right-style: solid; border-right-color: inherit; border-right-width: 1px; border-top-style: solid; border-top-color: inherit; border-top-width: 1px;">
