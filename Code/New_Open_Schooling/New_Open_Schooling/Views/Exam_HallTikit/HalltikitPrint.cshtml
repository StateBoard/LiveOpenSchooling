                                <span class="auto-style5">
                                    ADMISSION CARD FOR EVALUATION OF December - January - 2022 (STD - @Html.DisplayFor(model => model.Standard))
                                </span>
                            </td>
                        </tr>
                    </table>
                    <div style="border: 1px solid black; padding-top: 10px;">
                        <table border="0" cellspacing="2" cellpading="6" style="width: 101%; line-height: 20px;">
                            <tr>

                                <td colspan="4">
                                    <div>
                                        <div style="width: 70%; float: left">
                                            <table>
                                                <tr>
                                                    <td style="padding-left: 10px; font-weight: bold;" class="auto-style1">
